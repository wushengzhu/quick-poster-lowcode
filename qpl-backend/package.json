{
  "name": "qpl-backend",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-qpl-backend",
    "stop": "egg-scripts stop --title=egg-server-qpl-backend",
    "dev": "egg-bin dev --port 7001",
    "test:local": "egg-bin test -p",
    "test": "npm run lint -- --fix && npm run test:local",
    "cov": "egg-bin cov -p",
    "ci": "npm run lint && npm run cov && npm run tsc && npm run clean",
    "lint": "eslint . --ext .ts --cache",
    "tsc": "tsc",
    "clean": "tsc -b --clean",
    "build:template:dev": "npx webpack --config webpack/webpack.config.js",
    "build:template:prod": "npx webpack --config webpack/webpack.config.js --env production && npm run upload",
    "upload": "node webpack/uploadToOSS.js",
    "release": "release-it"
  },
